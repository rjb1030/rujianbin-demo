<?xml version="1.0" encoding="UTF-8"?>
<beans profile="qa" xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:tx="http://www.springframework.org/schema/tx" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
						http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd
						http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.2.xsd"
	default-lazy-init="true">

	<context:property-placeholder location="classpath*:xinyunlian.properties,
	                                     classpath*:env/qa/xinyunlian.properties,
				                         classpath*:env/qa/storePlugin.properties"
								  ignore-resource-not-found="true" ignore-unresolvable="true" />
	
</beans>